<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
         .gap
        {
            background-color: aqua;
            margin-left: 20px;
        }
        .btn
      {
        display: flex;
      margin-left: 100px;
      gap: 20px;
      margin-top: 0px;
      }
      .btc
      {
        border: 1px solid;
        background-color:beige;
        height: 30px;
        text-align: center;
        width: 50px;
   
      }
      a{
        text-decoration: none;
      }


      table
        {
            border: 1px solid ;
            width: 100%;
            text-align: center;
        }
        td
        {
            border: 1px solid;
        }
    </style>
</head>
<body>
        <table>
            <tr class="gap">
                <% for(const key in data[0]){%>
                    
                        <td><%= key %></td>
                    <% } %>
                        <td>action</td>
            
             </tr>
        <% data.forEach(element => { %>
              <tr>
                     <% for(const key in element){%>
                          <td><%= element[key] %></td>
                    <% } %>

                     <td> <a href="/updatedata?id=<%=element["canid"]%>">update</a></td>
                </tr>
         
        <% }) %>
        </table>
        </tr>
</body>
</html>